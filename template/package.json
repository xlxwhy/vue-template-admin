  "license": "MIT",
  "description": "A vue admin template with Element UI & axios & iconfont & permission control & lint",
  "author": "MWT",
  "scripts": {
    "dev": "cross-env port=9000 webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "dev:template": "cross-env port=8001 entry=template webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "dev:template2": "cross-env port=8002 entry=template2 webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "dev:group": "cross-env port=8000 entry=template,template2 webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "build": "node build/build.js",
    "build:template": "cross-env entry=template node build/build.js",
    "build:template2": "cross-env entry=template2 node build/build.js",
    "build:group": "cross-env entry=template,template2 node build/build.js",
    "dev0": "node build/dev-server.js",
    "start": "cd app && node app.js",
    "restart": "npm run build && npm run start",
    "build0": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "server": "node app/app.js"
  },
